{
    "type": "page",
    "page": [
        {
            "type": "header",
            "header": {
                "text": "Edit club",
                "buttons": [
                    {
                        "text": "Delete",
                        "confirm": "Are you sure?",
                        "target": "example/message.json"
                    }
                ]
            }
        },
        {
            "type": "form",
            "form": {
                "action": "example/clubs/store",
                "method": "get",
                "items": [
                    {
                        "type": "text",
                        "text": {
                            "name": "name",
                            "label": "Name of club",
                            "value": "The clicked club"
                        }                        
                    },
                    {
                        "type": "number",
                        "number": {
                            "name": "totalPlayers",
                            "label": "Number of players",
                            "min": "7",
                            "max": "25",
                            "value": "11"
                        }                        
                    },
                                        {
                        "type": "date",
                        "date": {
                            "name": "foundingdate",
                            "label": "Founded on",
                            "value": "1982-17-08"
                        }                        
                    },
                    {
                        "type": "range",
                        "range": {
                            "name": "range",
                            "label": "Shooting Range",
                            "min": "7",
                            "max": "25",
                            "value": "11"
                        }                        
                    },
                    {
                        "type": "email",
                        "email": {
                            "name": "email",
                            "label": "Club Email"
                        }                        
                    },
                      {
                        "type": "url",
                        "url": {
                            "name": "website",
                            "label": "Club Website",
                            "value": "http://www.voetbalclub.org"
                       }
                    },
                    {
                        "type": "tel",
                        "tel": {
                            "name": "telephone",
                            "label": "Club phone number",
                            "value": "11"
                        }                       
                    },
                    {
                        "type": "checkbox",
                        "checkbox": {
                            "name": "leagues",
                            "label": "Jupiler",
                            "required": false,
                            "checked": false
                        }
                        
                    },
                    {
                        "type": "selectMultipleCheckbox",
                        "selectMultipleCheckbox": {
                            "name": "beers",
                            "label": "Beers",
                            "required": false,
                            "options":
                            [
                                {
                                "type": "checkbox",
                                "label": "Jupiler",
                                "checked": false
                                },
                                {
                                "label": "Grolsch",
                                "checked": true
                                },
                                {
                                "label": "Heineken",
                                "checked": false
                                },
                                {
                                "label": "Duvel",
                                "checked": false
                                }
                           ]
                        }
                        
                    },
                     {
                        "type": "password",
                        "password": {
                            "name": "token",
                            "label": "Token"
                        }
                    },
                    {
                        "type": "textarea",
                        "textarea": {
                            "name": "description",
                            "value": "Something about this club",
                            "label": "Description"
                        }
                    },
                    {
                        "type": "submit",
                        "submit": {
                            "name": "Update"
                        }
                    }
                ]
            }
        }
    ]
}