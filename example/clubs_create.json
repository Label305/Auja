{
    "type": "page",
    "page": [
        {
            "type": "header",
            "header": {
                "contents": "Create club"
            }
        },
        {
            "type": "form",
            "form": {
                "action": "example/clubs/store",
                "method": "post",
                "items": [
                    {
                        "type": "text",
                        "text": {
                            "name": "name",
                            "label": "Name of club"
                        }
                    },
                    {
                        "type": "password",
                        "password": {
                            "name": "token",
                            "label": "Token"
                        }
                    },
                    {
                        "type": "textarea",
                        "textarea": {
                            "name": "description",
                            "label": "Description"
                        }
                    },
                    {
                        "type": "submit",
                        "submit": {
                            "name": "Create!"
                        }
                    }
                ]
            }
        }
    ]
}